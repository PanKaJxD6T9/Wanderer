<% layout("layouts/boilerplate.ejs") %>
<body>
    <h1>My Listings</h1>
    <form action="http://localhost:8080/listing/new" method="get">
        <button>Create New Listing</button>
    </form>
    <ul>
        <% for(listing of allListings) { %>
            <li><a href="/listing/<%= listing._id %>"><%= listing.title %></a></li>
        <% } %>
    </ul>
</body>